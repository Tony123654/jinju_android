<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45.0dip"
        android:background="@color/main_red" >

        <TextView
            style="@style/custom_title_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@color/white"
            android:text="@string/my_withfund_trans_password" />
        
        <ImageView
            android:id="@id/img_finish"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:padding="15dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:src="@mipmap/icon_cancel_white"/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/separator"/>
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp">
        
        <TextView
            android:id="@+id/txt_verify_name"
            style="@style/custom_word_black_middle_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/main_red"
            android:text="@string/bank_add_card"/>
        
        <TextView
            android:id="@+id/txt_bank_add_card"
            style="@style/custom_word_black_middle_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:textColor="@color/main_red"
            android:text="@string/sms_verify_code"/>
        
        <TextView
            android:id="@+id/txt_trans_password"
            style="@style/custom_word_black_middle_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:textColor="@color/main_red"
            android:text="@string/my_withfund_trans_password"/>
        
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt_verify_name"
            android:layout_marginLeft="30dp"
            android:layout_marginStart="30dp"
            android:layout_marginRight="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginTop="10dp">
            
            <ImageView 
	            android:id="@+id/img_verify_name"
	            android:layout_width="7dp"
	            android:layout_height="7dp"
	            android:layout_alignParentLeft="true"
	            android:layout_alignParentStart="true"
	            android:src="@mipmap/icon_red"/>

            <ImageView
	            android:id="@+id/img_bank_add_card"
	            android:layout_width="7dp"
	            android:layout_height="7dp"
	            android:layout_centerHorizontal="true"
	            android:src="@mipmap/icon_red"/>
            
            <ImageView 
	            android:id="@+id/img_trans_password"
	            android:layout_width="7dp"
	            android:layout_height="7dp"
	            android:layout_alignParentRight="true"
	            android:layout_alignParentEnd="true"
	            android:src="@mipmap/icon_red"/>
            
            <View 
	            android:layout_width="wrap_content"
	            android:layout_height="2dp"
	            android:layout_toRightOf="@id/img_verify_name"
	            android:layout_toEndOf="@id/img_verify_name"
	            android:layout_toLeftOf="@id/img_bank_add_card"
	            android:layout_toStartOf="@id/img_bank_add_card"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="5dp"
	            android:layout_marginRight="5dp"
	            android:background="@color/main_red"/>
            
            <View 
	            android:layout_width="wrap_content"
	            android:layout_height="2dp"
	            android:layout_toRightOf="@id/img_bank_add_card"
	            android:layout_toEndOf="@id/img_bank_add_card"
	            android:layout_toLeftOf="@id/img_trans_password"
	            android:layout_toStartOf="@id/img_trans_password"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="5dp"
	            android:layout_marginRight="5dp"
	            android:background="@color/main_red"/>
            
        </RelativeLayout>
    </RelativeLayout>
    
    <TextView
        style="@style/custom_word_black_middle_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="@string/confirm_trans_pwd"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="40dp"
        android:layout_marginRight="40dp"
        android:background="@drawable/six_box_bg"
        android:orientation="horizontal" >

        <TextView
            android:id="@id/txt_pay_box1"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@mipmap/six_boxline_bg" />

        <TextView
            android:id="@id/txt_pay_box2"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@mipmap/six_boxline_bg" />

        <TextView
            android:id="@id/txt_pay_box3"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@mipmap/six_boxline_bg" />

        <TextView
            android:id="@id/txt_pay_box4"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@mipmap/six_boxline_bg" />

        <TextView
            android:id="@id/txt_pay_box5"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@mipmap/six_boxline_bg" />

        <TextView
            android:id="@id/txt_pay_box6"
            style="@style/InputTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1" />
    </LinearLayout>
	<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:layout_alignParentBottom="true"
            android:background="#CFCFCF"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@id/img_keyboard_one"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_one_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_two"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_two_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_three"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_three_selector"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@id/img_keyboard_four"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_four_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_five"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_five_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_six"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_six_selector"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@id/img_keyboard_seven"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_seven_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_eight"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_marginLeft="1dp"
                    android:layout_marginRight="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_eight_selector" />

                <ImageView
                    android:id="@id/img_keyboard_nine"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_marginBottom="1dp"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_nine_selector"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@id/img_keyboard_space"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@mipmap/keyboard_space"/>

                <ImageView
                    android:id="@id/img_keyboard_zero"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_zero_selector"/>

                <ImageView
                    android:id="@id/img_keyboard_del"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@drawable/keyboard_del_selector"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>



</LinearLayout>